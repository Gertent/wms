<?xml version="1.0" encoding="UTF-8"?>  
<beans xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"    
    xsi:schemaLocation="  
        http://www.springframework.org/schema/beans   
        http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">  
  
    <bean id="instockBillView" class="com.rmd.wms.web.common.ApplicationIReportView">  
        <property name="url" value="/WEB-INF/jasper/installBill.jasper"/>  
        <property name="reportDataKey" value="jrMainDataSource"/>  
    </bean>  
    <bean id="deliveryBillView" class="com.rmd.wms.web.common.ApplicationIReportView">  
        <property name="url" value="/WEB-INF/jasper/deliveryBill.jasper"/>  
        <property name="reportDataKey" value="jrMainDataSource"/>  
    </bean> 
      <bean id="packBillView" class="com.rmd.wms.web.common.ApplicationIReportView">  
        <property name="url" value="/WEB-INF/jasper/packGoodsBill.jasper"/>  
        <property name="reportDataKey" value="jrMainDataSource"/>  
    </bean> 
    <bean id="inBoxBillView" class="com.rmd.wms.web.common.ApplicationIReportView">  
        <property name="url" value="/WEB-INF/jasper/inboxBill.jasper"/>  
        <property name="reportDataKey" value="jrMainDataSource"/>  
    </bean> 
    <bean id="checkBillView" class="com.rmd.wms.web.common.ApplicationIReportView">  
        <property name="url" value="/WEB-INF/jasper/checkBill.jasper"/>  
        <property name="reportDataKey" value="jrMainDataSource"/>  
    </bean> 
    <bean id="logisticsBillView" class="com.rmd.wms.web.common.ApplicationIReportView">  
        <property name="url" value="/WEB-INF/jasper/logisticsBill.jasper"/>  
        <property name="reportDataKey" value="jrMainDataSource"/>  
    </bean> 
</beans>  
